      <memberdef kind="function" id="group__NMSIS__Core__DSP__Intrinsic__SIMD__8B__CMP_1gaf3b7bef50684b91cf3b8de6045ea9579" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
